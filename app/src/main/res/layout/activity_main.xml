        app:layout_constraintTop_toTopOf="@+id/buttonUpgrade3"
        app:layout_constraintVertical_bias="0.482" />

    <TextView
        android:id="@+id/upgradeText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upgrade 4"
        app:layout_constraintBottom_toBottomOf="@+id/buttonUpgrade4"
        app:layout_constraintEnd_toStartOf="@+id/buttonUpgrade4"
